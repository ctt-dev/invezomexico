                  <xsd:enumeration value="Cedro"/>
                  <xsd:enumeration value="Chocolate"/>
                  <xsd:enumeration value="Dorado"/>
                  <xsd:enumeration value="Encino"/>
                  <xsd:enumeration value="Fresno"/>
                  <xsd:enumeration value="Fucsia"/>
                  <xsd:enumeration value="Gris"/>
                  <xsd:enumeration value="Lila"/>
                  <xsd:enumeration value="Madera"/>
                  <xsd:enumeration value="Morado"/>
