            <xsd:enumeration value="Bottom Mount"/>
            <xsd:enumeration value="Dúplex"/>
            <xsd:enumeration value="French Door"/>
            <xsd:enumeration value="Frigobar"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
