<?xml version="1.0" ?>
<xsd:schema elementFormDefault="qualified" targetNamespace="http://walmart.com/" vc:minVersion="1.1" version="3.2" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" xmlns:wm="http://walmart.com/" xmlns:xc="http://xerces.apache.org" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:include schemaLocation="MPCommons.xsd"/>
  <xsd:complexType name="Photography">
    <xsd:choice maxOccurs="1" minOccurs="0">
      <xsd:element name="CamerasAndLenses" type="wm:CamerasAndLenses"/>
      <xsd:element name="PhotoAccessories" type="wm:PhotoAccessories"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="CamerasAndLenses">
    <xsd:all>
      <xsd:element maxOccurs="1" minOccurs="0" name="lightingType">
        <xsd:annotation>
          <xsd:documentation>Especificar el tipo o tipos de efecto de luz que proporciona el accesorio de fotografía </xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Luz</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="400"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="maximumExpandableMemory">
        <xsd:annotation>
          <xsd:documentation>Esto representa la memoria expandible máxima para el elemento del teléfono celular en particular</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Memoria Expandible</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="200"/>
                  <xsd:minLength value="1"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="KB"/>
                  <xsd:enumeration value="MB"/>
                  <xsd:enumeration value="GB"/>
                  <xsd:enumeration value="TB"/>
                  <xsd:enumeration value="PB"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="subBrand">
        <xsd:annotation>
          <xsd:documentation>Indica la siguiente línea de importancia de la marca. Indica la sub-marca del producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Submarcas</wm:displayName>
            <wm:group>Basic</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="100"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="itemsIncluded">
        <xsd:annotation>
          <xsd:documentation>Describe los artículos que están incluidos en o con el producto. Los ejemplos pueden incluir hardware o accesorios.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Contenido del empaque</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
