          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Colibrí"/>
            <xsd:enumeration value="Canário"/>
            <xsd:enumeration value="Loro"/>
            <xsd:enumeration value="Cacatúa"/>
            <xsd:enumeration value="Pato"/>
