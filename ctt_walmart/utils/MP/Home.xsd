<?xml version="1.0" ?>
<xsd:schema elementFormDefault="qualified" targetNamespace="http://walmart.com/" vc:minVersion="1.1" version="3.2" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" xmlns:wm="http://walmart.com/" xmlns:xc="http://xerces.apache.org" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:include schemaLocation="MPCommons.xsd"/>
  <xsd:complexType name="Home">
    <xsd:choice maxOccurs="1" minOccurs="0">
      <xsd:element name="Bedding" type="wm:Bedding"/>
      <xsd:element name="LargeAppliances" type="wm:LargeAppliances"/>
      <xsd:element name="HomeOther" type="wm:HomeOther"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Bedding">
    <xsd:all>
      <xsd:element maxOccurs="1" minOccurs="0" name="pieceCount">
        <xsd:annotation>
          <xsd:documentation>Indicar el total de piezas que contiene el set y que llegarán al cliente.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Número de Piezas</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:totalDigits value="20"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="innerspringDesign">
        <xsd:annotation>
          <xsd:documentation>Indica la variación del resorte que cambia con el modelo de colchón.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Forma del Resorte</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Continuo"/>
            <xsd:enumeration value="Bolsillo (Marshal Coil)"/>
            <xsd:enumeration value="Bonell (Abiertos-Open coil)"/>
            <xsd:enumeration value="Compensación (Offset)"/>
            <xsd:enumeration value="Espuma Viscoelástica"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="secondaryMaterial">
        <xsd:annotation>
          <xsd:documentation>Refiere al elemento principal o predominante en la composición del producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Material de fabricación</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Acero"/>
            <xsd:enumeration value="Acero Carbonizado"/>
            <xsd:enumeration value="Acero Cromado"/>
            <xsd:enumeration value="Acero Inoxidable"/>
            <xsd:enumeration value="Acrílico"/>
            <xsd:enumeration value="Alambrón Cromado"/>
