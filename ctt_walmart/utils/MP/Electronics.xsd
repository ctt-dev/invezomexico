      <xsd:element maxOccurs="1" minOccurs="0" name="turntableCartridgeType">
        <xsd:annotation>
          <xsd:documentation>Indicar el diseño del brazo que soporta la cápsula. La forma del brazo busca hacer llegar de manera lo más paralelamente posible la capsula y su aguja al surco del disco.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Brazo</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="S"/>
            <xsd:enumeration value="Recto"/>
            <xsd:enumeration value="Tangencial"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="audioPowerAmplifierType">
        <xsd:annotation>
          <xsd:documentation>Indicar si cuenta con el semiconductor utilizado para la conmutación y amplificación de señales. (amplificador+potencia+bocinas)</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Amplificador</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="100"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="noiseCancelling">
        <xsd:annotation>
          <xsd:documentation>Indicar si cuenta con la tecnología que elimina los ruidos ambientales </xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Cancelación de Ruido</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="maximumExpandableMemory">
        <xsd:annotation>
          <xsd:documentation>Esto representa la memoria expandible máxima para el elemento del teléfono celular en particular</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Memoria Expandible</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="200"/>
                  <xsd:minLength value="1"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="KB"/>
                  <xsd:enumeration value="MB"/>
                  <xsd:enumeration value="GB"/>
                  <xsd:enumeration value="TB"/>
                  <xsd:enumeration value="PB"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="multimediaEnabled">
        <xsd:annotation>
          <xsd:documentation>Solo se aplica a los autoestéreos. Almacena, organiza y reproduce archivos de video, audio e imagen. Esto indica si el elemento es compatible con internet multimedia o no.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Internet Multimedia</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="isRemoteControlIncluded">
        <xsd:annotation>
          <xsd:documentation>¿Se incluye un control remoto con el artículo?</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Control Remoto</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="karaokeEnabled">
        <xsd:annotation>
          <xsd:documentation>El dispositivo cuenta o no con la opción de reproducir o reconocer los formatos de Karaoke.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Karaoke</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="radioSignalType">
        <xsd:annotation>
          <xsd:documentation> Indicar si se puede sintonizar estaciones AM/FM o cuenta con Radio Satelital</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Radio</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="AM/FM"/>
            <xsd:enumeration value="Satelital"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="audioTurntableDriveType">
        <xsd:annotation>
          <xsd:documentation>Indicar la trasmisión por la cual gira el plato </xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Transmisión</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Directa"/>
            <xsd:enumeration value="Por correa"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="operatingSystem" type="wm:OperatingSystem">
        <xsd:annotation>
          <xsd:documentation>Es el conjunto de órdenes y programas que controlan los procesos básicos de una computadora y permiten el funcionamiento de otros programas. Indicar el que corresponda al producto de la lista.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Sistema Operativo</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="turntablePlatterMatType">
        <xsd:annotation>
          <xsd:documentation>Indicar con que tipo de material está fabricado el plato.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Material del Plato</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Aluminio"/>
            <xsd:enumeration value="Cobre"/>
            <xsd:enumeration value="Cristal"/>
            <xsd:enumeration value="Cerámica"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="subwooferType">
        <xsd:annotation>
          <xsd:documentation>Indica el tamaño de la pantalla del producto en pulgadas.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Subwoofer</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Alámbrico"/>
            <xsd:enumeration value="Inalámbrico"/>
            <xsd:enumeration value="Integrado"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="frequency">
        <xsd:annotation>
          <xsd:documentation>Indicar la medición de onda en Hertz (Hz)</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Frecuencia</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                  <xsd:totalDigits value="400"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Hz"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="hasGps">
        <xsd:annotation>
          <xsd:documentation>Esto representa si el producto es compatible con GPS.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>GPS</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="isFoldable">
        <xsd:annotation>
          <xsd:documentation>Indica que un artículo puede ser plegado.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Plegable</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="size">
        <xsd:annotation>
          <xsd:documentation>Dimensiones generales de un artículo. Se usa solo para productos que aún no tienen un atributo más específico de &quot;tamaño x&quot;, como el tamaño del anillo o el tamaño de la ropa.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tamaño</wm:displayName>
            <wm:group>Discoverability</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="dvdEnabled">
        <xsd:annotation>
          <xsd:documentation>Indica si el producto cuenta con la funcionalidad de DVD.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>DVD</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="isReadyToUse">
        <xsd:annotation>
          <xsd:documentation>Indicar si el dispositivo informático puede ser conectado a una computadora sin tener que configurar.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Plug and Play</wm:displayName>
            <wm:group>Nice to Have</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="hasTouchscreen">
        <xsd:annotation>
          <xsd:documentation>Indicar si permite la interacción con el usuario a través de un toque</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Pantalla Táctil</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="hasMp3Player">
        <xsd:annotation>
          <xsd:documentation>Indica si el elemento del teléfono móvil es compatible con el reproductor de MP3 o no</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Reproductor MP3</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="hasOpticalConnection">
        <xsd:annotation>
          <xsd:documentation>The optical audio input is an optional port that is added to multimedia devices as a possibility plus connection and as an alternative to HDMI.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Entrada óptica</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="has3dCapabilities">
        <xsd:annotation>
          <xsd:documentation>Indicar si alguno de los formatos cuenta con esta tecnología</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>3D</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="lightBulbColor">
        <xsd:annotation>
          <xsd:documentation>Indicar la combinación de colores que despide el artículo</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Colores de la Luz</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="frontFacingCameraMegapixels">
        <xsd:annotation>
          <xsd:documentation>La resolución máxima de la cámara que enfrenta el usuario, proporcionada en megapíxeles.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Cámara</wm:displayName>
            <wm:group>Discoverability</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:totalDigits value="16"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="MP"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="watts">
        <xsd:annotation>
          <xsd:documentation>La cantidad de watts que utiliza el producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Potencia</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:totalDigits value="16"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="W"/>
                  <xsd:enumeration value="KW"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="material">
        <xsd:annotation>
          <xsd:documentation>El (los) material (es) principal (es) de los que está hecho un producto. Esto no necesita ser una lista exhaustiva, pero debe contener el material / materiales predominantes o funcionalmente importantes. Las especificaciones del material de la tela se deben ingresar usando el atributo &quot;Contenido de la tela&quot;.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>composición</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="compatibleSpeakerSizes" type="wm:CompatibleSpeakerSizes">
        <xsd:annotation>
          <xsd:documentation>Indicar el tamaño de la bocina para los autos</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tamaño De Bocina</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="computerSpeakerType">
        <xsd:annotation>
          <xsd:documentation>Indicar el tipo de bocina con la que cuenta el producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Bocinas</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Torre"/>
            <xsd:enumeration value="Satelital"/>
            <xsd:enumeration value="Portátil"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="detachableFaceplate">
        <xsd:annotation>
          <xsd:documentation>Indicar si la carátula del autoestereo es desmontable</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Carátula Desmontable</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="hasTripodMount">
        <xsd:annotation>
          <xsd:documentation>Indicar si cuenta con la manera de adaptarce a un tripíe </xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Entrada para Tripié</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="compatibleSoftwareApplications" type="wm:CompatibleSoftwareApplications">
        <xsd:annotation>
          <xsd:documentation>Indica el nombre de las aplicaciones o dispositivos compatibles con el producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Aplicaciones Compatibles</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="hasRadio">
        <xsd:annotation>
          <xsd:documentation>Indica si el producto tiene Radio FM.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Radio FM</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="totalHarmonicDistortion">
        <xsd:annotation>
          <xsd:documentation>Indicar el nivel de alteración en herts (Número + Hz)</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Distorsión</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="100"/>
                  <xsd:minLength value="1"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Hz"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="audioVideoCableType">
        <xsd:annotation>
          <xsd:documentation>Indicar qué tipo de cable es; MIDI; Jack Estéreo; RCA Macho a 2 RCA hembra; etc</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de cable</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="MIDI"/>
            <xsd:enumeration value="Mini Jack"/>
            <xsd:enumeration value="RCA Macho"/>
            <xsd:enumeration value="RCA hembra"/>
            <xsd:enumeration value="XLR Macho"/>
            <xsd:enumeration value="XLR Hembra"/>
            <xsd:enumeration value="TRS"/>
            <xsd:enumeration value="TS"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="dimensions">
        <xsd:annotation>
          <xsd:documentation>Colocar las medidas alto, longitud y ancho del artículo SIN EMPAQUE en CENTÍMETROS. (AltoxLongitudxAncho)</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Dimensiones</wm:displayName>
            <wm:group>Discoverability</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="turntableCartridgeStylusType">
        <xsd:annotation>
          <xsd:documentation>Indicar qué tipo de aguja cuenta el equipo para la reproduccción de los discos </xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Aguja</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Elíptica"/>
            <xsd:enumeration value="Esférica"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="interfaceType">
        <xsd:annotation>
          <xsd:documentation>La forma de conexión con la que cuenta el producto, que puede conllevar el uso de un cable, de manera inalámbrica o con ambas.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Conectividad de Red</wm:displayName>
            <wm:group>Discoverability</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="1" name="interfaceTypeValue">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Ethernet"/>
                  <xsd:enumeration value="Inalámbrica"/>
                  <xsd:enumeration value="Inalámbrica y Ethernet"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="volts">
        <xsd:annotation>
          <xsd:documentation>Número de voltios que el producto produce o requiere. La importancia del voltaje varía con el tipo de producto. Por ejemplo, si el producto es un aparato, se utiliza para describir el voltaje de salida requerido. Si un elemento (como una fuente de alimentación) produce y requiere diferentes voltajes, se debe incluir cada tipo de voltaje.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Voltaje</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:totalDigits value="16"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="mV"/>
                  <xsd:enumeration value="V"/>
                  <xsd:enumeration value="VAC"/>
                  <xsd:enumeration value="VDC"/>
                  <xsd:enumeration value="kV"/>
                  <xsd:enumeration value="kVAC"/>
                  <xsd:enumeration value="kVDC"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="multiroom">
        <xsd:annotation>
          <xsd:documentation>Indicar si cuenta con la función de  reproducción de música en cualquier parte de tu casa. Esto es posible gracias a la interconexión de dispositivos de audio a través de una misma red WiFi y que normalmente se pueden manejar desde una app móvil y/o de escritorio.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Multiroom</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="headphoneStyle">
        <xsd:annotation>
          <xsd:documentation>El estilo de los auriculares está destinado a la especificación de la interfaz auditiva entre los auriculares y el usuario. Puede ser personaje, DJ, moda, oreja, oreja, sobre oreja o deporte</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Audífonos</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Diadema"/>
            <xsd:enumeration value="Deportivos"/>
            <xsd:enumeration value="In Ear"/>
            <xsd:enumeration value="DJ"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="signalToNoiseRatio">
        <xsd:annotation>
          <xsd:documentation>Indicar la unidad de medida de intensidad sonora (Número)</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Relación Señal/Ruido</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                  <xsd:totalDigits value="10"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="db"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="numberOfBatteries">
        <xsd:annotation>
          <xsd:documentation>Se indica la cantidad de pilas que utiliza el artículo para su funcionamiento.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Número de Baterías</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:totalDigits value="7"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="hasNightVision">
        <xsd:annotation>
          <xsd:documentation>Indica si el producto cuenta con esta funcionalidad.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Visión Nocturna</wm:displayName>
            <wm:group>Discoverability</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="percentageOfEnergySavings">
        <xsd:annotation>
          <xsd:documentation>Indica el porcentaje de ahorro de energía del producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Ahorro de energía</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:decimal">
            <xsd:totalDigits value="8"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="numberOfUsers">
        <xsd:annotation>
          <xsd:documentation>Indica el número de personas que pueden tener acceso; conexión o se pueden ingresar en el directorio.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Capacidad de Usuarios</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:totalDigits value="7"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="rmsPowerRating">
        <xsd:annotation>
          <xsd:documentation>Se indica la potencia del producto del listado.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Potencia RMS</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:totalDigits value="4000"/>
                  <xsd:enumeration value="100"/>
                  <xsd:enumeration value="500"/>
                  <xsd:enumeration value="1000"/>
                  <xsd:enumeration value="1500"/>
                  <xsd:enumeration value="2000"/>
                  <xsd:enumeration value="2500"/>
                  <xsd:enumeration value="3000"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Watts"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="hasFingerprintReader">
        <xsd:annotation>
          <xsd:documentation>Indicar si el producto cuenta con lector de huella o no</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Lector de Huella</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="lightingType">
        <xsd:annotation>
          <xsd:documentation>Especificar el tipo o tipos de efecto de luz que proporciona el accesorio de fotografía </xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Luz</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="400"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="hasMotionDetector">
        <xsd:annotation>
          <xsd:documentation>Menciona si el artículo posee esta funcionalidad.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Motion Tracking</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="dvdRegionCode">
        <xsd:annotation>
          <xsd:documentation>Se indica la region compatible con el producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Region</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="400"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="accessoriesIncluded" type="wm:AccessoriesIncluded">
        <xsd:annotation>
          <xsd:documentation>Indica la familia a la que pertenece el accesorio </xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Accesorio</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="customerAssistance" type="wm:CustomerAssistance">
        <xsd:annotation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Contacto de Asistencia</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="powerType">
        <xsd:annotation>
          <xsd:documentation>Indicación del tipo de corriente que utiliza el producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Corriente</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Continua"/>
            <xsd:enumeration value="Alterna"/>
            <xsd:enumeration value="Universal"/>
            <xsd:enumeration value="Directa"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="isSmart">
        <xsd:annotation>
          <xsd:documentation>Indica si un dispositivo se encuentra entre el conjunto de dispositivos &quot;inteligentes&quot; (que indica que la tecnología web, inalámbrica o interactiva se ha integrado en la funcionalidad del dispositivo).</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Smart</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="maximumTotalWattage" type="wm:PowerUnit">
        <xsd:annotation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Consumo en Watts</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="graphicsInformation">
        <xsd:annotation>
          <xsd:documentation>Número de pixeles que pueden ser mostrados en la pantalla, dado por el producto del ancho por el alto. Seleccionar el que se adecúe al producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Gráficos</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="1080p (FHD)"/>
            <xsd:enumeration value="720p (HD)"/>
            <xsd:enumeration value="2160 (4K)"/>
            <xsd:enumeration value="4320 (8K)"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="batteryLife">
        <xsd:annotation>
          <xsd:documentation>Indica la duración aproximada del artículo en funcionamiento, en horas.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Duración de la Batería</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:totalDigits value="16"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="h"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="onScreenDisplay">
        <xsd:annotation>
          <xsd:documentation>Se indica si el producto cuenta con panel para controlar las funciones.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Panel De Control</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="shortDescription">
        <xsd:annotation>
          <xsd:documentation>Descripción general de los puntos de venta clave del artículo, contenido de marketing y puntos destacados en forma de párrafo. Para fines de SEO, repita el nombre del producto y las palabras clave relevantes aquí.</xsd:documentation>
