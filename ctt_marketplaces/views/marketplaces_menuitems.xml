<odoo>
    <data>
        <record model="ir.actions.act_window" id="action_marketplaces_list">
            <field name="name">Marketplaces</field>
            <!-- <field name="type">ir.actions.act_window</field> -->
            <field name="res_model">marketplaces.marketplace</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_marketplaces_categories">
            <field name="name">Categorías</field>
            <!-- <field name="type">ir.actions.act_window</field> -->
            <field name="res_model">marketplaces.category</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'search_default_marketplace_group': 1}</field>
            <field name="domain">[('is_installed', '=', True)]</field>
            <field name="search_view_id" ref="view_marketplaces_category_search"/>
        </record>
        
        <menuitem id="menu_marketplaces" name="Marketplaces" parent="stock.menu_stock_root" sequence="9"/>
        <menuitem id="menu_marketplaces_List" 
            action="action_marketplaces_list"
            name="Marketplaces" 
            parent="menu_marketplaces" 
            sequence="1"/>
        <menuitem id="menu_marketplaces_categories" 
            action="action_marketplaces_categories"
            name="Categorías" 
            parent="menu_marketplaces" 
            sequence="2"/>
    </data>
</odoo>