  <xsd:complexType name="PrintersScannersAndImaging">
    <xsd:all>
      <xsd:element maxOccurs="1" minOccurs="0" name="printerCartridgeType">
        <xsd:annotation>
          <xsd:documentation>Los dos tipos de cartuchos que se pueden encontrar comercialmente para las impresores de tinta y de toner. Seleccionar el correspondiente.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Cartucho</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Toner"/>
            <xsd:enumeration value="Tinta"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
