            <xsd:enumeration value="Volteadores y Pinzas"/>
            <xsd:enumeration value="Cortadores y Cuchillos"/>
            <xsd:enumeration value="Soportes y Rejillas"/>
            <xsd:enumeration value="Guantes y Fundas"/>
            <xsd:enumeration value="Planchas y Paelleras"/>
            <xsd:enumeration value="Desbrozadora"/>
            <xsd:enumeration value="Podadora"/>
            <xsd:enumeration value="Desmalezadora"/>
            <xsd:enumeration value="Nylon"/>
            <xsd:enumeration value="Botas"/>
            <xsd:enumeration value="Escobas y Rastrillos"/>
            <xsd:enumeration value="Tijeras"/>
            <xsd:enumeration value="Palas y Picos"/>
