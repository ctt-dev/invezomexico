<odoo>
    <data>
        
        <!-- VISTA FORM -->
        <record model="ir.ui.view" id="l10n_mx_cfdi_fiel_form">
            <field name="name">cfdi fiel form</field>
            <field name="model">l10n_mx.cfdi_fiel</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="fiel_name" invisible="1"/>
                            <!-- <field name="fiel" string="FIEL (.key)" filename="fiel_name" widget="binary" required="1"/> -->
                            <field name="fiel" string="Key (.key)" filename="fiel_name" widget="binary" required="1"/>
                            <field name="clave_name" invisible="1"/>
                            <!-- <field name="clave" string="Clave (.cer)" filename="clave_name" widget="binary" required="1"/> -->
                            <field name="clave" string="Certificate (.cer)" filename="clave_name" widget="binary" required="1"/>
                            <field name="company_id" readonly="1" save_force="1"/>
                            <!-- <field name="serial_number" string="ContraseÃ±a" password="True" required="1"/> -->
                            <field name="serial_number" string="Password" password="True" required="1"/>
<!--                             <label for="emition_date" string="Fechas"/> -->
                        </group>
                        <!-- <group string="Fechas"> -->
                        <group string="Dates">
                            <group>
                                <field name="emition_date" readonly="1" force_save="1"/>
                            </group>
                            <group>
                                <field name="expiration_date" readonly="1" force_save="1"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
                <!-- VISTA FORM -->
        <record model="ir.ui.view" id="l10n_mx_cfdi_fiel_tree">
            <field name="name">cfdi fiel tree</field>
            <field name="model">l10n_mx.cfdi_fiel</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="company_id"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>