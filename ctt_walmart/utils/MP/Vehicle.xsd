      <xsd:element maxOccurs="1" minOccurs="0" name="generalStandardsAndCertifications">
        <xsd:annotation>
          <xsd:documentation>Norma de seguridad que aprueba al casco.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
