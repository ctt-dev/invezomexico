            <xsd:enumeration value="Alambrón Plateado"/>
            <xsd:enumeration value="Algodón"/>
            <xsd:enumeration value="Algodón Egipcio"/>
            <xsd:enumeration value="Aluminio"/>
            <xsd:enumeration value="Bambú"/>
            <xsd:enumeration value="Cable Metálico"/>
            <xsd:enumeration value="Cartón"/>
            <xsd:enumeration value="Caucho"/>
            <xsd:enumeration value="Cedro"/>
            <xsd:enumeration value="Cera"/>
            <xsd:enumeration value="Cerámica"/>
            <xsd:enumeration value="Cristal"/>
            <xsd:enumeration value="Espuma"/>
            <xsd:enumeration value="Espuma de Poliuretano"/>
            <xsd:enumeration value="Espuma Visco Elástica"/>
            <xsd:enumeration value="Hierro fundido"/>
            <xsd:enumeration value="Hule Espuma"/>
            <xsd:enumeration value="Madera"/>
            <xsd:enumeration value="Madera de Pino"/>
            <xsd:enumeration value="Hule Natural"/>
            <xsd:enumeration value="Mármol"/>
            <xsd:enumeration value="Material Reciclado"/>
            <xsd:enumeration value="Melamina"/>
            <xsd:enumeration value="Memory Foam"/>
            <xsd:enumeration value="Metal"/>
            <xsd:enumeration value="Metal Cromado"/>
            <xsd:enumeration value="Microfibra"/>
            <xsd:enumeration value="Nylon"/>
            <xsd:enumeration value="Opalglass"/>
            <xsd:enumeration value="Papel"/>
            <xsd:enumeration value="Percal"/>
            <xsd:enumeration value="Plástico"/>
            <xsd:enumeration value="Plástico ABS"/>
            <xsd:enumeration value="Plástico EVA"/>
            <xsd:enumeration value="Plástico Libre de BPA"/>
            <xsd:enumeration value="Pluma de Ganso"/>
            <xsd:enumeration value="Policarbonato"/>
            <xsd:enumeration value="Polietileno"/>
            <xsd:enumeration value="Polipropileno"/>
            <xsd:enumeration value="Poliéster"/>
            <xsd:enumeration value="Poliresina"/>
            <xsd:enumeration value="Poliuretano"/>
            <xsd:enumeration value="Porcelana"/>
            <xsd:enumeration value="PVC"/>
            <xsd:enumeration value="Fibras Sintéticas"/>
            <xsd:enumeration value="Resina"/>
            <xsd:enumeration value="Silicón"/>
            <xsd:enumeration value="Tela"/>
            <xsd:enumeration value="Titanio"/>
            <xsd:enumeration value="Vidrio"/>
            <xsd:enumeration value="Vidrio Forrado de Metal"/>
            <xsd:enumeration value="Vidrio Templado"/>
            <xsd:enumeration value="Vinil"/>
            <xsd:enumeration value="Vitroceramica Acero"/>
            <xsd:enumeration value="Acero Inoxidable"/>
            <xsd:enumeration value="Acero Vitrificado"/>
            <xsd:enumeration value="Acrílico"/>
            <xsd:enumeration value="Alambrón Cromado"/>
            <xsd:enumeration value="Alambrón Plateado"/>
            <xsd:enumeration value="Algodón"/>
            <xsd:enumeration value="Aluminio"/>
            <xsd:enumeration value="Bambú"/>
            <xsd:enumeration value="Cable metálico"/>
            <xsd:enumeration value="Cartón"/>
            <xsd:enumeration value="Caucho"/>
            <xsd:enumeration value="Cedro"/>
            <xsd:enumeration value="Cera"/>
            <xsd:enumeration value="Cerámica"/>
            <xsd:enumeration value="Cristal"/>
            <xsd:enumeration value="Hierro fundido"/>
            <xsd:enumeration value="Madera"/>
            <xsd:enumeration value="Mármol"/>
            <xsd:enumeration value="Material Reciclado"/>
            <xsd:enumeration value="Melamina"/>
            <xsd:enumeration value="Metal"/>
            <xsd:enumeration value="Microfibra"/>
            <xsd:enumeration value="Nylon"/>
            <xsd:enumeration value="Opalglass"/>
            <xsd:enumeration value="Papel"/>
            <xsd:enumeration value="Plástico"/>
            <xsd:enumeration value="Plástico ABS"/>
            <xsd:enumeration value="Plástico Libre de BPA"/>
            <xsd:enumeration value="Policarbonato"/>
            <xsd:enumeration value="Poliéster"/>
            <xsd:enumeration value="Polietileno"/>
            <xsd:enumeration value="Polipropileno"/>
            <xsd:enumeration value="Poliuretano"/>
            <xsd:enumeration value="Porcelana"/>
            <xsd:enumeration value="PVC"/>
            <xsd:enumeration value="Resina"/>
            <xsd:enumeration value="Silicón"/>
            <xsd:enumeration value="Titanio"/>
            <xsd:enumeration value="Vidrio"/>
            <xsd:enumeration value="Vidrio Templado"/>
            <xsd:enumeration value="Vitrelle"/>
            <xsd:enumeration value="Vitroacero"/>
            <xsd:enumeration value="Vitrocerámica Plástico"/>
