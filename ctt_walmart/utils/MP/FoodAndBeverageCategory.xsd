                <xsd:restriction base="xsd:decimal">
                  <xsd:totalDigits value="8"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="kcal"/>
                  <xsd:enumeration value="cal"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="sugarPerServing">
        <xsd:annotation>
          <xsd:documentation>Indicar el contenido de azucares totales del alimento, con la unidad de medida que se encuentra en el empaque. Ejemplo: 14 kcal</xsd:documentation>
