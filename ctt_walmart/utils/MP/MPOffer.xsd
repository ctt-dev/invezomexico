          <xsd:documentation>Indica si el artículo es o contiene material peligroso conforme al catálogo de Material Peligroso del SAT para Carta Porte.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>¿Es material peligroso?</wm:displayName>
            <wm:group>Offer</wm:group>
            <wm:rank>31000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="hazardousMaterialsTaxCode">
        <xsd:annotation>
          <xsd:documentation>Clave del material peligroso del SAT para Carta Porte que corresponda a la mercancía ofertada.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Clave del material peligroso</wm:displayName>
            <wm:rank>32000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
	<xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{4,4}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
</xsd:schema>
