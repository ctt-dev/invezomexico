            <wm:requiredLevel value="Required"/>
            <wm:displayName>Link de imágenes secundarias</wm:displayName>
            <wm:group>Images</wm:group>
            <wm:rank>13000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
