                  <xsd:enumeration value="Conteos"/>
                  <xsd:enumeration value="Música"/>
                  <xsd:enumeration value="Lectura"/>
                  <xsd:enumeration value="Escritura"/>
                  <xsd:enumeration value="Creatividad"/>
