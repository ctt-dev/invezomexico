<?xml version="1.0" ?>
<xsd:schema elementFormDefault="qualified" targetNamespace="http://walmart.com/" vc:minVersion="1.1" version="3.2" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" xmlns:wm="http://walmart.com/" xmlns:xc="http://xerces.apache.org" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:include schemaLocation="MPCommons.xsd"/>
  <xsd:complexType name="ToysCategory">
    <xsd:choice maxOccurs="1" minOccurs="0">
      <xsd:element name="Toys" type="wm:Toys"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Toys">
    <xsd:all>
      <xsd:element maxOccurs="1" minOccurs="0" name="watts">
        <xsd:annotation>
          <xsd:documentation>La cantidad de watts que utiliza el producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Potencia</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:totalDigits value="16"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="W"/>
                  <xsd:enumeration value="KW"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="rollerSkateType">
        <xsd:annotation>
          <xsd:documentation>Indica el estilo del producto que varía en cantidad de ruedas, posición y adaptabilidad al zapato.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Patines</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="En línea"/>
            <xsd:enumeration value="4 ruedas"/>
            <xsd:enumeration value="Patín armable"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="modelScale">
        <xsd:annotation>
          <xsd:documentation>En autos de colección, se identifica la escala con la que cuenta el artículo. </xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Escala</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="isRemoteControlIncluded">
        <xsd:annotation>
          <xsd:documentation>¿Se incluye un control remoto con el artículo?</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Control Remoto</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="compatibleSoftwareApplications" type="wm:CompatibleSoftwareApplications">
        <xsd:annotation>
          <xsd:documentation>Indica el nombre de las aplicaciones o dispositivos compatibles con el producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Aplicaciones Compatibles</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="batteryLife">
        <xsd:annotation>
          <xsd:documentation>Indica la duración aproximada del artículo en funcionamiento, en horas.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Duración de la Batería</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="measure">
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:totalDigits value="16"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="unit">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="h"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="clayAndDoughType">
        <xsd:annotation>
          <xsd:documentation>Se indica el tipo de material moldeable. </xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Plastilina</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Plastilina"/>
            <xsd:enumeration value="Arena"/>
            <xsd:enumeration value="Arcilla"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="wheelDiameter" type="wm:LengthUnit">
        <xsd:annotation>
          <xsd:documentation>Tamaño de la llanta de las bicicletas, referente al diámetro.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Rodada</wm:displayName>
            <wm:group>Discoverability</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="subBrand">
        <xsd:annotation>
          <xsd:documentation>Indica la siguiente línea de importancia de la marca. Indica la sub-marca del producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Submarcas</wm:displayName>
            <wm:group>Basic</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="100"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="collection">
        <xsd:annotation>
          <xsd:documentation>Identifica la familia o serie a la que pertenece el producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Colección</wm:displayName>
            <wm:group>Nice to Have</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="isBpaFree">
        <xsd:annotation>
          <xsd:documentation>Especificar si es o no libre de BPA (tóxico).</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Libre de BPA</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="modelName">
        <xsd:annotation>
          <xsd:documentation>El nombre del modelo con el que se refiere el producto.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Modelo</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="electronicsFeature">
        <xsd:annotation>
          <xsd:documentation>Identifica si el artículo cuenta con funciones como grabar, emitir sonidos, girar, prender o similar.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Electrónico</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Inverter"/>
            <xsd:enumeration value="Portátil"/>
            <xsd:enumeration value="De ventana"/>
            <xsd:enumeration value="Aire lavado"/>
            <xsd:enumeration value="On/Off"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="productLine">
        <xsd:annotation>
          <xsd:documentation>Identifica en los juguetes de construcción el escenario al que pertenece el set.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Mundo</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="400"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="isArticulated">
        <xsd:annotation>
          <xsd:documentation>Indicar si la figura de acción o Muñecas cuenta con movilidad, puntos de acción.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Articulada</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Sí"/>
            <xsd:enumeration value="No"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="warningText">
        <xsd:annotation>
          <xsd:documentation>Indicar los avisos de protección que el cliente debe tomar en cuenta para el uso del artículo</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Leyendas de Protección / Advertencias</wm:displayName>
            <wm:group>Compliance</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="wheelType">
        <xsd:annotation>
          <xsd:documentation>Indica la clase de rueda con la que cuenta el producto</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Tipo de Rueda</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="shoeSize">
        <xsd:annotation>
          <xsd:documentation>Refiere al número de calzado, con especificación de la nacionalidad.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Required"/>
            <wm:displayName>Talla de Calzado</wm:displayName>
            <wm:group>Discoverability</wm:group>
            <wm:rank>None</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="12 (MX)"/>
            <xsd:enumeration value="12.5 (MX)"/>
            <xsd:enumeration value="13 (MX)"/>
            <xsd:enumeration value="13.5 (MX)"/>
            <xsd:enumeration value="14 (MX)"/>
